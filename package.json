    "dev:client": "vite",
    "dev:server": "node --loader tsx src/server/index.ts",
    "dev:server:watch": "tsx watch src/server/index.ts",
    "build": "tsc && vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "build:extension": "NODE_ENV=development node scripts/build-extension.js",
    "build:extension:prod": "NODE_ENV=production node scripts/build-extension.js",
    "preview": "vite preview",
    "start": "node dist/server/index.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "postinstall": "prisma generate || true"
