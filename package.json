{
  "name": "website-cloner-pro",
  "version": "1.0.0-minimal",
  "description": "Lightweight Website Cloning & Performance Optimization Tool",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "node --loader tsx src/server/index.ts",
    "build": "tsc && vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "preview": "vite preview",
    "start": "node dist/server/index.js",
    "install:full": "cp package.full.json package.json && npm install"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.25.0",
    "archiver": "^7.0.1",
    "axios": "^1.6.7",
    "cheerio": "^1.1.2",
    "clean-css": "^5.3.3",
    "clsx": "^2.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "css": "^3.0.0",
    "css-tree": "^3.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-session": "^1.18.2",
    "file-type": "^21.0.0",
    "html-minifier-terser": "^7.2.0",
    "jsdom": "^27.0.0",
    "jsonwebtoken": "^9.0.2",
    "jszip": "^3.10.1",
    "lucide-react": "^0.344.0",
    "morgan": "^1.10.1",
    "nanoid": "^5.1.6",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "recharts": "^2.15.4",
    "svgo": "^3.2.0",
    "tailwind-merge": "^2.2.1",
    "terser": "^5.44.0",
    "web-vitals": "^3.5.2",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/clean-css": "^4.2.11",
    "@types/cors": "^2.8.17",
    "@types/css": "^0.0.38",
    "@types/express": "^4.17.21",
    "@types/html-minifier-terser": "^7.0.2",
    "@types/jsdom": "^27.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.19.0",
    "typescript": "^5.3.3",
    "vite": "^5.4.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
